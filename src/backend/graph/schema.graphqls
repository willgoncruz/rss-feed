type Feed {
    id: String!
    url: String!
    link: String!
    title: String!
    author: Author!
    published: String!
}

type Author {
    uri: String!
    name: String!
}

type StarRating {
    count: Int!
    average: Float!
    min: Int!
    max: Int!
}

type Statistics {
    views: Int!
}

type Community {
    starRating: StarRating!
    statistics: Statistics!
}

type Content {
    url: String!
    type: String!
    width: Int!
    height: Int!
}

type Thumbnail {
    url: String!
    width: Int!
    height: Int!
}

type Media {
    title: String!
    content: Content!
    thumbnail: Thumbnail!
    description: String!
    community: Community!
}

type Article {
    id: String!
    type: String!
    link: String!
    media: Media!
    title: String!
    author: Author!
    updated: String!
    published: String!
}

type Query {
    feeds: [Feed]
    articles(url: String): [Article]
}

type Mutation {
  removeElement(id: ID): Boolean
}